[Unit]
Description=uWSGI instance tu up SABU-GUI
After=network.target

[Service]
User=sabu
Group=sabu
WorkingDirectory=/sabu/gui/
ExecStart=python3 server.py
StandardOutput=append:/sabu/logs/gui.log
StandardError=append:/sabu/logs/gui.log
